{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.wysiwyg": false,
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "validTypes",
      "options": [
        "page",
        "blog",
        "portfolio"
      ]
    }
  ],
  "frontMatter.data.folders": [
    {
      "id": "authors",
      "path": "[[workspace]]/data/authors",
      "type": "author",
      "singleEntry": true
    }
  ],
  "frontMatter.data.types": [
    {
      "id": "authors",
      "schema": {
        "title": "Authors",
        "type": "object",
        "required": [
          "name",
          "url"
        ],
        "properties": {
          "name": {
            "title": "Name",
            "type": "string"
          },
          "url": {
            "title": "URL",
            "type": "string"
          },
          "slug": {
            "title": "Slug",
            "type": "string",
            "single": true
          }
        }
      }
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Content Preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "category",
          "type": "categories"
        },
        {
          "title": "Image",
          "name": "image",
          "type": "string",
          "single": true
        }
      ]
    },
    {
      "name": "page",
      "pageBundle": false,
      "fields": [
        {
          "title": "Page Title",
          "name": "title",
          "type": "string",
          "required": true
        },
        {
          "title": "Written Date",
          "name": "date",
          "hidden": true,
          "type": "datetime"
        },
        {
          "title": "Heading",
          "name": "heading",
          "type": "string",
          "single": true
        },
        {
          "title": "Type",
          "name": "type",
          "type": "taxonomy",
          "taxonomyId": "validTypes",
          "taxonomyLimit": 1,
          "default": "page",
          "hidden": true,
          "editable": false,
          "required": true
        }
      ]
    },
    {
      "name": "portfolio",
      "pageBundle": false,
      "fields": [
        {
          "title": "Article Title",
          "name": "title",
          "type": "string",
          "required": true
        },
        {
          "title": "Written Date",
          "name": "date",
          "hidden": true,
          "type": "datetime"
        },
        {
          "title": "Publishing Date",
          "name": "publish_date",
          "type": "datetime",
          "hidden": true,
          "isPublishDate": true
        },
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "single": true
        },
        {
          "title": "Image",
          "name": "image",
          "type": "string",
          "single": true
        },
        {
          "title": "Project Images",
          "name": "project_images",
          "type": "list"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "taxonomy",
          "taxonomyId": "validTypes",
          "taxonomyLimit": 1,
          "default": "{{pathToken.3}}",
          "hidden": true,
          "editable": false,
          "required": true
        },
        {
          "title": "Page Slug",
          "name": "slug",
          "type": "string",
          "editable": false,
          "default": "{{slug}}",
          "hidden": true
        },
        {
          "title": "Categories",
          "name": "category",
          "type": "categories"
        }
      ]
    },
    {
      "name": "blog",
      "pageBundle": false,
      "fields": [
        {
          "title": "Post Title",
          "name": "title",
          "type": "string",
          "required": true
        },
        {
          "title": "Written Date",
          "name": "date",
          "hidden": true,
          "type": "datetime"
        },
        {
          "title": "Publishing Date",
          "name": "publish_date",
          "type": "datetime",
          "hidden": true,
          "isPublishDate": true
        },
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "single": true
        },
        {
          "title": "Post Image",
          "name": "image",
          "type": "image",
          "single": true
        },
        {
          "title": "Banner Image",
          "name": "feature_image",
          "type": "image",
          "single": true
        },
        {
          "title": "Type",
          "name": "type",
          "type": "taxonomy",
          "taxonomyId": "validTypes",
          "taxonomyLimit": 1,
          "default": "{{pathToken.3}}",
          "hidden": true,
          "editable": false,
          "required": true
        },
        {
          "title": "Page Slug",
          "name": "slug",
          "type": "string",
          "editable": false,
          "default": "{{slug}}",
          "hidden": true
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.file.preserveCasing": false,
  "frontMatter.content.defaultFileType": ".md",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/content",
      "title": "content"
    },
    {
      "title": "about",
      "path": "[[workspace]]/content/about",
      "contentTypes": [
        "page"
      ]
    },
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog",
      "previewPath": "/{{fm.type}}/{{fm.slug}}",
      "contentTypes": [
        "blog"
      ]
    },
    {
      "title": "contact",
      "path": "[[workspace]]/content/contact",
      "contentTypes": [
        "page"
      ]
    },
    {
      "title": "portfolio",
      "path": "[[workspace]]/content/portfolio",
      "contentTypes": [
        "portfolio"
      ]
    }
  ],
  "frontMatter.taxonomy.tags": [],
  "frontMatter.taxonomy.categories": [],
  "frontMatter.content.autoUpdateDate": false,
  "frontMatter.framework.startCommand": "hugo server -D --navigateToChanged -p 6969",
  "frontMatter.preview.host": "http://localhost:6969/foodsin/"
}