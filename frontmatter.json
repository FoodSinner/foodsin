{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.wysiwyg": false,
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "validTypes",
      "options": [
        "page",
        "blog",
        "portfolio"
      ]
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Content Preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "category",
          "type": "categories"
        },
        {
          "title": "Image",
          "name": "image",
          "type": "string",
          "single": true
        }
      ]
    },
    {
      "name": "blog",
      "pageBundle": false,
      "fields": [
        {
          "title": "Post Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Image",
          "name": "image",
          "type": "string",
          "single": true
        },
        {
          "title": "Feature Image",
          "name": "feature_image",
          "type": "string",
          "single": true
        },
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "single": true
        },
        {
          "title": "Type",
          "name": "type",
          "type": "taxonomy",
          "taxonomyId": "validTypes"
        },
        {
          "title": "Page Slug",
          "name": "slug",
          "editable": false,
          "default": "{{slug}}",
          "hidden": true
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.file.preserveCasing": true,
  "frontMatter.content.defaultFileType": ".md",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/content",
      "title": "content"
    },
    {
      "title": "about",
      "path": "[[workspace]]/content/about",
      "contentTypes": [
        "page"
      ]
    },
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog",
      "contentTypes": [
        "blog"
      ]
    },
    {
      "title": "contact",
      "path": "[[workspace]]/content/contact",
      "contentTypes": [
        "page"
      ]
    },
    {
      "title": "portfolio",
      "path": "[[workspace]]/content/portfolio",
      "originalPath": "[[workspace]]/content/portfolio",
      "contentTypes": [
        "portfolio"
      ]
    }
  ],
  "frontMatter.taxonomy.tags": [],
  "frontMatter.taxonomy.categories": [],
  "frontMatter.content.autoUpdateDate": false,
  "frontMatter.framework.startCommand": "hugo server -D --navigateToChanged -p 6969",
  "frontMatter.preview.host": "http://localhost:6969/"
}